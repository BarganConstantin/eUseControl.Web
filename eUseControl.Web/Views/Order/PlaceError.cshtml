
@{
    ViewBag.Title = "PlaceError";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model eUseControl.Web.Models.UserData
<!-- Thank You area start -->
<div class="thank-you-area">
    <div class="container">
        <div class="row justify-content-center align-items-center">
            <div class="col-md-8">
                <div class="inner_complated">
                    <p class="dsc_cmpted">
                        <h3>Some products are not in enough stock</h3>
                    </p>
                    <p class="dsc_cmpted">
                        @Model.MsgStatus
                    </p>
                    <div class="btn_cmpted">
                        <a href="@Url.Action("Index", "Home")" class="shop-btn" title="Go To Shop">Continue Shopping </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Thank You area end -->
